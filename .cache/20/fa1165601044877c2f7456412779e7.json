\ndiv.question-box {\n  /* border: 1px dashed black;*/\n  padding: 9px 3px 3px 3px;\n  width: 70vw;\n  margin-top: 0px;\n  margin-bottom: 10px;\n  border-radius: 2px;\n  transition: background-color 0.5s;\n  background-color: #fcfcfc;\n  box-sizing: border-box;\n}\n\nh6 {\n  font-size: 1.05em;\n  font-weight: bold;\n  margin-top: 2px;\n  margin-bottom: 7px;\n  margin-left: 10px;\n  padding-left: 7px;\n  border-left: 2px solid #a22c29;\n}\n\ndiv.question-box:hover {\n  /* transition: all 0.5s; */\n  background-color: #ededed;\n}\n\n/* QUESTTION-CHOICE STYLING */\nlabel {\n  display: block;\n  padding: 5px;\n  box-sizing: border-box;\n}\n\ndiv.single-choice-box {\n  display: flex;\n  flex-flow: row nowrap;\n  align-items: center;\n  transition: all 0.5s;\n  margin: 0px;\n  padding-top: 5px;\n  padding-bottom: 5px;\n  border-radius: 2px;\n}\n\ndiv.single-choice-box:hover {\n  background-color: #fcfcfc;\n}\n\nlabel:hover {\n  cursor: pointer;\n}\n\np {\n  margin: 0;\n}\n\n/* RADIO BUTTON STYLING */\n\ninput[type=\"radio\"] {\n  align-items: center;\n  border: 2px solid lime;\n  display: none;\n}\n\nspan.input-box {\n  position: relative;\n  border: 2px solid #6d9dc5;\n  width: 12px;\n  height: 12px;\n  margin-left: 8px;\n  margin-right: 8px;\n  border-radius: 2px;\n  transition: transform 0.02s;\n  box-sizing: border-box;\n}\n\nspan.input-box:after {\n  content: \"\";\n  display: inline-block;\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  width: 13px;\n  height: 13px;\n  transform: translate(-50%, -50%) scale(0);\n  transition: transform 0.1s;\n  background-color: #004e98;\n  border-radius: 2px;\n  box-sizing: border-box;\n}\n\ninput[type=\"radio\"]:checked ~ span.input-box:after {\n  transform: translate(-50%, -50%) scale(1);\n}\n\n/* SETTING ORDER OF FLEX (SINGLE-CHOICE-BOX) ELEMENTS */\ndiv.single-choice-box :nth-child(1) {\n  order: 2;\n}\n\ndiv.single-choice-box :nth-child(2) {\n  order: 3;\n  flex: 3;\n}\n\ndiv.single-choice-box :nth-child(3) {\n  order: 1;\n}\n\n/* CHECK&RESTART BUTTON STYLING w/ RESULTBOX*/\n\ndiv > button.submit-quiz,\ndiv > button.restart-quiz {\n  box-sizing: border-box;\n  border: none;\n  border-radius: 3px;\n  background-color: #EDEDED;\n  font-size: 1.2em;\n  font-weight: 900;\n  padding: 13px;\n  margin-bottom: 10px;\n  transition: background-color 0.3s;\n  flex-grow: 1;\n  cursor: pointer;\n  outline: none;\n}\n\ndiv > button.restart-quiz {\n  margin-right: 13px;\n}\n\ndiv > button.restart-quiz:disabled {\n  background-color: #fcfcfc;\n  cursor: not-allowed;\n}\n\ndiv > button.submit-quiz:disabled:hover {\n  background-color: #fcfcfc;\n  cursor: not-allowed;\n}\n\ndiv > button.submit-quiz:hover,\nbutton.restart-quiz:hover {\n  background-color: #FCFCFC;\n}\n\n/* STYLING RESULT BOX BUTTON */\n\ndiv.result {\n  box-sizing: border-box;\n  border: none;\n  border-radius: 3px;\n  background-color: #fcfcfc;\n  font-size: 1.2em;\n  font-weight: bold;\n  padding: 13px;\n  width: 70vw;\n  margin-bottom: 10px;\n  text-align: center;\n}\n\n/* MEDIA QUIERIES */\n\n@media (max-width: 900px) {\n  form {\n    width: 100%;\n    padding: 0 0px;\n    box-sizing: border-box;\n  }\n\n  div.question-box {\n    width: 100%;\n    background-color: #ededed;\n    /* border: 1px dashed lime; */\n  }\n\n  div.quiz-buttons-cont {\n    flex-flow: column nowrap;\n    box-sizing: border-box;\n    width: 100%;\n    /* border: 1px dashed lime; */\n  }\n\n  div > button.restart-quiz {\n    margin-right: 0px;\n    margin-bottom: 10px;\n  }\n\n  div.result {\n    width: 100%;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"5f3e24322bd1fea62b39c2a5d4bfa5fa","cacheData":{"env":{}}}