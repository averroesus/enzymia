nerated":{"line":1,"column":2993}},{"source":"question_style.css","name":null,"original":{"line":273,"column":2},"generated":{"line":1,"column":2994}},{"source":"question_style.css","name":null,"original":{"line":273,"column":18},"generated":{"line":1,"column":3009}},{"source":"question_style.css","name":null,"original":{"line":274,"column":2},"generated":{"line":1,"column":3010}},{"source":"question_style.css","name":null,"original":{"line":274,"column":20},"generated":{"line":1,"column":3027}},{"source":"question_style.css","name":null,"original":{"line":275,"column":2},"generated":{"line":1,"column":3028}},{"source":"question_style.css","name":null,"original":{"line":275,"column":14},"generated":{"line":1,"column":3039}},{"source":"question_style.css","name":null,"original":{"line":277,"column":2},"generated":{"line":1,"column":3040}},{"source":"question_style.css","name":null,"original":{"line":277,"column":35},"generated":{"line":1,"column":3071}},{"source":"question_style.css","name":null,"original":{"line":278,"column":2},"generated":{"line":1,"column":3072}},{"source":"question_style.css","name":null,"original":{"line":279,"column":0},"generated":{"line":1,"column":3096}},{"source":"question_style.css","name":null,"original":{"line":281,"column":0},"generated":{"line":1,"column":3097}},{"source":"question_style.css","name":null,"original":{"line":282,"column":2},"generated":{"line":1,"column":3126}},{"source":"question_style.css","name":null,"original":{"line":282,"column":38},"generated":{"line":1,"column":3150}},{"source":"question_style.css","name":null,"original":{"line":283,"column":2},"generated":{"line":1,"column":3151}},{"source":"question_style.css","name":null,"original":{"line":284,"column":0},"generated":{"line":1,"column":3165}}],"sources":{"question_style.css":"/* QUESTTION FORM STYLING */\n\nform {\n  margin: 0;\n}\n\n/* QUESTIONBOX STYLING */\n\ndiv.question-box {\n  /* border: 1px dashed black;*/\n  padding: 9px 3px 3px 3px;\n  width: 70vw;\n  margin-top: 0px;\n  margin-bottom: 6px;\n  border-radius: 2px;\n  transition: background-color 0.5s;\n  background-color: #fcfcfc;\n  box-sizing: border-box;\n}\n\nh6 {\n  font-size: 1.05em;\n  font-weight: normal;\n  margin-top: 2px;\n  margin-bottom: 7px;\n  margin-left: 10px;\n  padding-left: 7px;\n  border-left: 2px solid #a22c29;\n}\n\ndiv.question-box:hover {\n  /* transition: all 0.5s; */\n  background-color: #ededed;\n}\n\n/* QUESTTION-CHOICE STYLING */\nlabel {\n  display: block;\n  padding: 5px;\n  box-sizing: border-box;\n}\n\ndiv.single-choice-box {\n  display: flex;\n  flex-flow: row nowrap;\n  align-items: center;\n  transition: all 0.5s;\n  margin: 0px;\n  padding-top: 5px;\n  padding-bottom: 5px;\n  border-radius: 2px;\n}\n\ndiv.single-choice-box:hover {\n  background-color: #fcfcfc;\n}\n\nlabel:hover {\n  cursor: pointer;\n}\n\np {\n  margin: 0;\n}\n\n/* RADIO BUTTON STYLING */\n\ninput[type=\"radio\"] {\n  align-items: center;\n  border: 2px solid lime;\n  display: none;\n}\n\nspan.input-box {\n  position: relative;\n  border: 2px solid #6d9dc5;\n  width: 12px;\n  height: 12px;\n  margin-left: 8px;\n  margin-right: 8px;\n  border-radius: 2px;\n  transition: transform 0.02s;\n  box-sizing: border-box;\n}\n\nspan.input-box:after {\n  content: \"\";\n  display: inline-block;\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  width: 13px;\n  height: 13px;\n  transform: translate(-50%, -50%) scale(0);\n  transition: transform 0.1s;\n  background-color: #004e98;\n  border-radius: 2px;\n  box-sizing: border-box;\n}\n\ninput[type=\"radio\"]:checked ~ span.input-box:after {\n  transform: translate(-50%, -50%) scale(1);\n}\n\n/* SETTING ORDER OF FLEX (SINGLE-CHOICE-BOX) ELEMENTS */\ndiv.single-choice-box :nth-child(1) {\n  order: 2;\n}\n\ndiv.single-choice-box :nth-child(2) {\n  order: 3;\n  flex: 3;\n}\n\ndiv.single-choice-box :nth-child(3) {\n  order: 1;\n}\n\n/* CHECK&RESTART BUTTON STYLING w/ RESULTBOX*/\n\ndiv > button.submit-quiz,\ndiv > button.restart-quiz {\n  box-sizing: border-box;\n  border: none;\n  border-radius: 3px;\n  background-color: #EDEDED;\n  font-size: 1.2em;\n  font-weight: 900;\n  padding: 13px;\n  transition: background-color 0.3s;\n  flex-grow: 1;\n  cursor: pointer;\n  outline: none;\n}\n\ndiv > button.restart-quiz {\n  margin-right: 6px;\n}\n\ndiv > button.submit-quiz {\n  margin-bottom: 0;\n}\n\ndiv > button.restart-quiz:disabled {\n  background-color: #fcfcfc;\n  cursor: not-allowed;\n}\n\ndiv > button.submit-quiz:disabled:hover {\n  background-color: #fcfcfc;\n  cursor: not-allowed;\n}\n\ndiv > button.submit-quiz:hover,\nbutton.restart-quiz:hover {\n  background-color: #FCFCFC;\n}\n\n/* STYLING RESULT BOX BUTTON */\n\ndiv.result {\n  box-sizing: border-box;\n  border: none;\n  border-radius: 3px;\n  background-color: #fcfcfc;\n  font-size: 1.2em;\n  font-weight: bold;\n  padding: 13px;\n  width: 70vw;\n  margin-bottom: 10px;\n  text-align: center;\n}\n\n/* MEDIA QUIERIES */\n\n@media (max-width: 900px) {\n  form {\n    width: 100%;\n    padding: 0 0px;\n    box-sizing: border-box;\n  }\n\n  div.question-box {\n    width: 100%;\n    background-color: #ededed;\n    /* border: 1px dashed lime; */\n  }\n\n  div.quiz-buttons-cont {\n    flex-flow: column nowrap;\n    box-sizing: border-box;\n    width: 100%;\n    /* border: 1px dashed lime; */\n  }\n\n  div > button.restart-quiz {\n    margin-right: 0px;\n    margin-bottom: 6px;\n  }\n\n  div.result {\n    width: 100%;\n  }\n}\n\n/* STYLING MAIN ALERT DIV BLOCK FOR INNER STRUCTURING  */\n\ndiv.info-main {\n  position: fixed;\n\n  display: flex;\n  flex-flow: column nowrap;\n  align-items: center;\n  justify-content: center;\n\n  width: 100%;\n  height: 100%;\n}\n\n/* STYLING A.CAL ONLICK ALERT WINDOW  */\n\ndiv.hover {\n  position: absolute;\n  display: flex;\n  flex-flow: column nowrap;\n  align-items: center;\n  justify-content: center;\n\n  height: 100%;\n  width: 100%;\n  background-color: rgba(237, 237, 237, 0.7);\n\n  transition: background-color 0.3s;\n}\n\n/* STYLING A.CAL ONLICK INFO-BOX ALERT WINDOW  */\n\ndiv.info-box {\n  position: absolute;\n\n  display: flex;\n  flex-flow: column nowrap;\n  justify-content: center;\n\n  height: auto;\n  width: 50%;\n\n  border-radius: 4px;\n  border: 2px solid #dbdbdb;\n  padding: 10px;\n  background-color: #ededed;\n\n  transition: background-color 0.1s;\n}\n\n/* STYLING INFO-BOX INNER TEXT P ELEMENT  */\n\np.info-box-text {\n  font-family: Helvetica, sans-serif;\n  font-weight: 500;\n  font-size: 0.8em;\n\n  line-height: 2em;\n  text-align: center;\n\n  margin: 0;\n}\n\n/* STYLING INFO-BOX BUTTON ELEMENT  */\n\nbutton.info-box-button {\n  border: none;\n  margin-top: 16px;\n  border-radius: 2px;\n  padding: 6px;\n\n  transition: background-color 0.3s;\n  background-color: #dfdfdf;\n}\n\nbutton.info-box-button:hover {\n  background-color: rgb(250, 250, 250);\n  cursor: pointer;\n}\n"},"lineCount":null}},"error":null,"hash":"70baa0702f3f039f87823617a6a1dedf","cacheData":{"env":{}}}