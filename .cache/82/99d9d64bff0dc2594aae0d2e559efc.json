-------- */\n\nnav.quiz-list {\n    display: flex;\n    flex-flow: column nowrap;\n    align-items: left;\n  }\n  \ndiv.list-item {\n    display: flex;\n    flex-flow: row nowrap;\n    justify-content: left;\n\n    /* THIS IS A VERTICAL ALIGN CAUSE FLEX = ROW  */\n    /* align-items: center; */\n\n    margin-bottom: 6px;\n\n}\n\n/* MAKING LAST LIST ITEM WITHOUT ANY BORDER */\n\ndiv.list-item:last-of-type {\n    margin-bottom: 0px;\n}\n\n/* ----------------------- */\n\n/* STYLING A LINK ON MAIN PAGE  */\n\ndiv.list-item a {\n    display: flex;\n    flex: row auto;\n    align-items: center;\n\n    height: auto;\n    padding: 6px;\n    margin-right: 6px;\n\n    border-radius: 2px;\n    text-decoration: none;\n    color: #020402;\n    transition: background-color 0.3s;\n    background-color: #ededed;\n    line-height: 1em;\n}\n\nimg {\n    height: 1em;\n    width: 1em;\n}   \n\n/* ----------------------- */\n\n/* MOVE LAST ITEM IN DIV.LIST-ITEM FLEX TO RIGHT */\n\ndiv.list-item a.quiz  {\n    margin-left: auto; \n    margin-right: 0;\n}\n\ndiv.list-item *:hover  {\n    background-color: #fcfcfc;\n    cursor: pointer;\n}\n\n/* ----------------------- */\n\n/* STYLING NOT-READY <A> LIST ITEMS  */\n\ndiv.list-item a.not-ready {\n    cursor: not-allowed;\n    transition: background-color 0.3s;\n}\n\ndiv.list-item a.not-ready:hover, \na.cal:hover, \na.progress:hover {\n    background-color: #F3CFCE;\n}\n\n/* ------------------------ */\n\n\n/* SCREEN SIZE FOR IPHONE */\n\n@media screen and (max-width: 325px) {\n    div.list-item a {\n        line-height: 1.3em;\n    }\n}\n\n/* STYLES FROM JS */\n\n/* STYLING MAIN ALERT DIV BLOCK FOR INNER STRUCTURING  */\n\ndiv.info-main {\n    position: absolute;\n\n    display: flex;\n    flex-flow: column nowrap;\n    align-items: center;\n    justify-content: center;\n\n    width: 100%;\n    height: 100%;\n}\n\n/* STYLING A.CAL ONLICK ALERT WINDOW  */\n\ndiv.hover {\n    position: absolute;\n    display: flex;\n    flex-flow: column nowrap;\n    align-items: center;\n    justify-content: center;\n\n    height: 100%;\n    width: 100%;\n    background-color: rgba(237, 237, 237, 0.7);\n\n    transition: background-color 0.3s;\n}\n\n/* STYLING A.CAL ONLICK INFO-BOX ALERT WINDOW  */\n\ndiv.info-box {\n    position: absolute;\n\n    display: flex;\n    flex-flow: column nowrap;\n    justify-content: center;\n\n    height: auto;\n    width: 50%;\n\n    border-radius: 4px;\n    border: 2px solid #dbdbdb;\n    padding: 10px;\n    background-color: #ededed;\n\n    transition: background-color 0.1s;\n}\n\n/* STYLING INFO-BOX INNER TEXT P ELEMENT  */\n\np.info-box-text {\n    font-family: Verdana, Geneva, Tahoma, sans-serif;\n    font-weight: 500;\n    font-size: 0.8em;\n\n    line-height: 2em;\n    text-align: center;\n\n    margin: 0;\n}\n\n/* STYLING INFO-BOX BUTTON ELEMENT  */\n\nbutton.info-box-button {\n    border: none;\n    margin-top: 16px;\n    border-radius: 2px;\n    padding: 6px;\n\n    transition: background-color 0.3s;\n    background-color: #dfdfdf;\n}\n\nbutton.info-box-button:hover {\n    background-color: rgb(250, 250, 250);\n    cursor: pointer;\n}"},"lineCount":174}},"error":null,"hash":"b5c0180a62819b043d71586b76c2ca87","cacheData":{"env":{}}}