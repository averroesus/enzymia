e":54,"column":32},"source":"../js/main.js","original":{"line":79,"column":8}},{"generated":{"line":55,"column":0},"source":"../js/main.js","original":{"line":80,"column":7}},{"generated":{"line":56,"column":0},"source":"../js/main.js","original":{"line":81,"column":5}},{"generated":{"line":57,"column":0},"source":"../js/main.js","original":{"line":82,"column":3}},{"generated":{"line":57,"column":3},"source":"../js/main.js","original":{"line":51,"column":2}},{"generated":{"line":58,"column":0},"source":"../js/main.js","original":{"line":83,"column":1}},{"generated":{"line":58,"column":1},"source":"../js/main.js","original":{"line":13,"column":0}},{"name":"reqQuiz","generated":{"line":60,"column":0},"source":"../js/main.js","original":{"line":85,"column":0}},{"generated":{"line":60,"column":7},"source":"../js/main.js","original":{"line":85,"column":7}},{"name":"send","generated":{"line":60,"column":8},"source":"../js/main.js","original":{"line":85,"column":8}},{"generated":{"line":60,"column":12},"source":"../js/main.js","original":{"line":85,"column":0}}],"sources":{"../js/main.js":"const form = document.querySelector(\"form\");\nconst checkBtn = document.querySelector(\"button[type=submit]\");\nconst restartBtn = document.querySelector(\"button.restart-quiz\");\nconst resultBox = document.querySelector(\".result\");\n\nconsole.log(resultBox);\n\n// LOAD QUIZ FOROM .JSON FILE\nconst reqQuiz = new XMLHttpRequest();\n\nreqQuiz.open(\"GET\", \"/quizes(json)/iodine.json\");\n\nreqQuiz.onload = function () {\n  const quizContent = JSON.parse(reqQuiz.responseText);\n  // console.log(quizContent);\n\n  //DISABLE QUIZ RESTARTING BUTTON\n  restartBtn.disabled = true;\n\n  let outputQ = \"\";\n\n  //CREATE QUIZBOX WITH ANSWERS INSIDE IT\n  quizContent.forEach(function (questions, qNumber) {\n    // console.log(questions.answers);\n\n    outputQ += `<div class=\"question-box\">\n    <h6> ${questions.questionName} </h6>\n    `;\n\n    //GET ANSWERS IDs\n    let outputA = \"\";\n    for (letter in questions.answers) {\n      outputA += `<label>\n                    <div class='single-choice-box'>\n                      <input type='radio' name='${\n                        qNumber + 1\n                      }' value='${letter}' id='${letter}'>\n                        <p> ${questions.answers[letter]} </p>\n                      <span class=\"input-box\"> </span>\n                    </div>\n                  </label>\n                  `;\n    }\n\n    outputQ += `<div class='choices-box'> ${outputA} </div>`;\n    outputQ += `</div>`;\n    form.innerHTML = outputQ;\n  });\n\n  //COUNT THE AMOUNT OF CORRECT ANSWERS\n  checkBtn.addEventListener(\"click\", function (e) {\n    e.preventDefault();\n\n    countTrue = 0;\n    countFalse = 0;\n\n    let availableAnswers = document.querySelectorAll(\"div.question-box\");\n    let selectedAnswers = document.querySelectorAll(\n      \"input[type=radio]:checked\"\n    );\n\n    //CHECK IF ALL QUESTIONS ARE ANSWERED\n    if (availableAnswers.length !== selectedAnswers.length) {\n      window.alert(\"Пожалуйста, ответьте на все вопросы\");\n    } else {\n      //LOOP THROUGH THIS CHOICES\n      for (let i = 0; i < selectedAnswers.length; i++) {\n        if (selectedAnswers[i].value === quizContent[i].correct) {\n          countTrue += 1;\n        } else {\n          countFalse += 1;\n        }\n\n        //SHOW FINAL RESULT --- CORRECT AND INCORRECT ANSWERS INCLUDED\n        let output = `Ваш результат ${countTrue} из ${availableAnswers.length}.`;\n        resultBox.innerHTML = output;\n\n        restartBtn.disabled = false;\n        checkBtn.disabled = true;\n      }\n    }\n  });\n};\n\nreqQuiz.send();\n"},"lineCount":null}},"error":null,"hash":"279f82ba7578fb7082d44ec0c8a6470e","cacheData":{"env":{}}}